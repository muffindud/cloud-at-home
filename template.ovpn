client
dev tun
proto udp
remote %REMOTE_HOST% %REMOTE_PORT%
resolv-retry infinite
nobind
persist-key
persist-tun
remote-cert-tls server
auth SHA512
ignore-unknown-option block-outside-dns
verb 3

<cert>
%CERT%
</cert>

<key>
# Insert your private key here
</key>

<ca>
%CA%
</ca>

<tls-crypt>
%TLS_CRYPT%
</tls-crypt>

